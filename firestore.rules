rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /solicitud_presupuesto/{document} {
      allow create: if true;
    }
    match /solicitud_presupuesto/{document} {
      allow read: if request.auth != null;
    }
  }
  
  match /solicitud_express/{document} {
      allow create: if true;
    }
    match /solicitud_express/{document} {
      allow read: if request.auth != null;
    }
  }